{
  "name": "htw-berlin-ecco-webapp",
  "description": "HTW Berlin: Explore Chemical Characteristics of the Ocean",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:leaflet.glify && ng serve",
    "build:leaflet.glify": "cd node_modules/leaflet.glify/ && npm install && npm run build && cd ../../",
    "build:fastify": "tsc --project server.tsconfig.json",
    "build": "npm run build:leaflet.glify && ng build --prod && npm run build:fastify",
    "test": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/pwa": "^0.11.0",
    "@angular/router": "^7.1.1",
    "@angular/service-worker": "^7.1.1",
    "@datorama/akita": "^1.22.1",
    "@datorama/akita-ng-router-store": "^1.0.2",
    "@mdi/font": "^3.2.89",
    "@swimlane/ngx-charts": "^10.0.0",
    "command-line-args": "^5.0.2",
    "core-js": "^2.5.4",
    "fastify": "^1.13.1",
    "fastify-static": "^0.14.0",
    "hammerjs": "^2.0.8",
    "heatmap.js": "^2.0.5",
    "leaflet": "^1.3.4",
    "leaflet.glify": "git+https://github.com/robertleeplummerjr/Leaflet.glify.git",
    "lodash": "^4.17.11",
    "ngx-take-until-destroy": "^5.2.3",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.0",
    "@angular/cli": "^7.1.0",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@datorama/akita-ngdevtools": "^1.0.2",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.11",
    "codelyzer": "^4.5.0",
    "cypress": "^3.1.2",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "^3.1.6"
  }
}
